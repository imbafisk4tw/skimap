{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Skimap travel_times.json schema",
  "type": "object",
  "required": ["homeKey", "updatedAt", "times"],
  "additionalProperties": true,
  "properties": {
    "homeKey": { "type": "string", "minLength": 1 },
    "updatedAt": { "type": "string", "minLength": 1 },
    "times": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": ["seconds"],
        "additionalProperties": true,
        "properties": {
          "seconds": { "type": "number", "minimum": 0 },
          "source": { "type": "string" },
          "updatedAt": { "type": "string" }
        }
      }
    }
  }
}
